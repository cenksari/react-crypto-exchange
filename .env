# Development Environment Configuration
VITE_APP_NAME=Crypto Exchange Dev
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_WS_BASE_URL=ws://localhost:8000/ws

# Authentication
VITE_JWT_SECRET=dev_jwt_secret_change_in_production
VITE_REFRESH_TOKEN_EXPIRY=7d
VITE_ACCESS_TOKEN_EXPIRY=15m

# Feature Flags
VITE_ENABLE_2FA=false
VITE_ENABLE_KYC=false
VITE_ENABLE_TRADING=true

# Third-party Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Security
VITE_CSP_NONCE=
VITE_CSRF_TOKEN=

# Performance
VITE_ENABLE_SERVICE_WORKER=false
VITE_CACHE_DURATION=0

# Development
VITE_DEBUG_MODE=true
VITE_MOCK_API=true
